{
  "expo": {
    "name": "check-in",
    "slug": "check-in",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/worker-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/worker-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/worker-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.teslicvukasin.checkin"
    },
    "web": {
      "favicon": "./assets/worker-logo.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "01d0ea0e-5270-4a25-914c-bb9ffab4a3df"
      }
    }
  }
}
